	/// Partition listing all hierarchy level options across calendar types
	partition SlicerDateValues = calculated
		mode: import
		source = ```
				{ 
				        // Gregorian/Calendar hierarchy levels 
				        ("CalendarYear",    NAMEOF('Date'[CalendarYear]),        0, "Calendar", 0, "Calendar Year"), 
				        ("CalendarQuarter", NAMEOF('Date'[CalendarYearQuarter]), 1, "Calendar", 0, "Calendar Quarter"), 
				        ("CalendarMonth",   NAMEOF('Date'[CalendarYearMonth]),   2, "Calendar", 0, "Calendar Month"), 
				
				        // Fiscal hierarchy levels 
				        ("FiscalYear",    NAMEOF('Date'[FiscalYear]),        3, "Fiscal", 1, "Fiscal Year"), 
				        ("FiscalQuarter", NAMEOF('Date'[FiscalYearQuarter]), 4, "Fiscal", 1, "Fiscal Quarter"), 
				        ("FiscalPeriod",  NAMEOF('Date'[FiscalYearPeriod]),  5, "Fiscal", 1, "Fiscal Period"), 
				        ("FiscalWeek",    NAMEOF('Date'[FiscalYearWeek]),    6, "Fiscal", 1, "Fiscal Week"), 
				
				        // ISO week (ISO 8601 calendar) 
				        ("ISOWeek", NAMEOF('Date'[ISOYearWeek]), 7, "ISO", 2, "ISO Week"), 
				
				        // Retail 4-4-5 calendar hierarchy levels 
				        ("RetailYear",    NAMEOF('Date'[RetailYear]),        8, "Retail", 3, "Retail Year"), 
				        ("RetailQuarter", NAMEOF('Date'[RetailYearQuarter]), 9, "Retail", 3, "Retail Quarter"), 
				        ("RetailPeriod",  NAMEOF('Date'[RetailYearPeriod]),  10, "Retail", 3, "Retail Period"), 
				
				        // Lunar calendar hierarchy levels 
				        ("LunarYear",  NAMEOF('Date'[LunarYear]),      11, "Lunar", 4, "Lunar Year"), 
				        ("LunarMonth", NAMEOF('Date'[LunarYearMonth]), 12, "Lunar", 4, "Lunar Month"), 
				
				        // 13-Month calendar hierarchy levels 
				        ("13MonthYear",   NAMEOF('Date'[Period13MYear]),        13, "13-Month", 5, "13-Month Year"), 
				        ("13MonthPeriod", NAMEOF('Date'[Period13MYearPeriod]), 14, "13-Month", 5, "13-Month Period"), 
				
				        // Date (individual date level) 
				        ("Date", NAMEOF('Date'[Date]), 15, "Date", 6, "Date") 
				    } 
				```

